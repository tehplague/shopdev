{
    "name" : "jtl/shop4",
    "type" : "app",
    "license" : "proprietary",
    "require" : {
        "silex/silex" : "~1.2",
        "predis/service-provider" : "~0.4",
        "php" : ">=5.4.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~4.1",
        "symfony/browser-kit": "~2.5",
        "symfony/css-selector": "~2.5"
    },
    "minimum-stability" : "alpha",
    "repositories" : [
        {
            "type": "git",
            "url": "git@gitlab.com:jtl-software-gmbh/shop4-backend.git",
            "reference": "origin/master"
        }
    ],
    "config" : {
        "bin-dir" : "bin"
