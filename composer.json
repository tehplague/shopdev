    "autoload" : {
        "psr-4" : {
            "Jtl\\Shop4\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Jtl\\Shop4\\Tests\\" : "tests/"
        }
    },
    "authors" : [
        {
            "name" : "Christian Spoo",
